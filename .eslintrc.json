{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "new-cap": 0,
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "camelcase": "off",
        "no-throw-literal": "off",
        "max-len": "off"
    }
}
